{
  "manifest_version": 2,

  "name": "The Most Terrible Extension",
  "description": "Make every image display a marvelous and majestic cat!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Replacer"
  },
  "permissions":[
    "tabs"
  ],
  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery3.js","background.js"],
    "run_at": "document_end"
  }
]
}
